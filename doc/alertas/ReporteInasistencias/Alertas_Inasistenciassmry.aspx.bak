	' Return Sort parameters based on Sort links clicked
	Public Function GetSort() As String
		Dim bCtrl As Boolean = False
		Dim sOrder As String, sOrderType As String, sOrderBy As String
		Dim sSortField As String, sLastSort As String, sThisSort As String = "", sSortSql As String = ""

		' Check for a resetsort command
		If ew_NotEmpty(ew_Get("cmd")) Then
			Dim sCmd As String = ew_Get("cmd")
			If ew_SameText(sCmd, "resetsort") Then
				Alertas_Inasistencias.OrderBy = ""
				Alertas_Inasistencias.StartGroup = 1
				Alertas_Inasistencias.CODCLI.Sort = ""
				Alertas_Inasistencias.RUT.Sort = ""
				Alertas_Inasistencias.NombreAlumno.Sort = ""
				Alertas_Inasistencias.Fecha.Sort = ""
				Alertas_Inasistencias.Alerta.Sort = ""
				Alertas_Inasistencias.CodAsignatura.Sort = ""
				Alertas_Inasistencias.NombreAsignatura.Sort = ""
				Alertas_Inasistencias.Seccion.Sort = ""
				Alertas_Inasistencias.Semestre.Sort = ""
				Alertas_Inasistencias.NombreProfesor.Sort = ""
				Alertas_Inasistencias.CODCARRERA.Sort = ""
				Alertas_Inasistencias.NOMBRECARRERA.Sort = ""
				Alertas_Inasistencias.CODPLAN.Sort = ""
				Alertas_Inasistencias.Mailenviado.Sort = ""
				Alertas_Inasistencias.email.Sort = ""
			End If

		' Check for an Order parameter
		ElseIf (ew_NotEmpty(ew_Get("order"))) Then
			Alertas_Inasistencias.CurrentOrder = ew_Get("order")
			Alertas_Inasistencias.CurrentOrderType = ew_Get("ordertype")
			Alertas_Inasistencias.UpdateSort(Alertas_Inasistencias.CODCLI) ' CODCLI
			Alertas_Inasistencias.UpdateSort(Alertas_Inasistencias.RUT) ' RUT
			Alertas_Inasistencias.UpdateSort(Alertas_Inasistencias.NombreAlumno) ' NombreAlumno
			Alertas_Inasistencias.UpdateSort(Alertas_Inasistencias.Fecha) ' Fecha
			Alertas_Inasistencias.UpdateSort(Alertas_Inasistencias.Alerta) ' Alerta
			Alertas_Inasistencias.UpdateSort(Alertas_Inasistencias.CodAsignatura) ' CodAsignatura
			Alertas_Inasistencias.UpdateSort(Alertas_Inasistencias.NombreAsignatura) ' NombreAsignatura
			Alertas_Inasistencias.UpdateSort(Alertas_Inasistencias.Seccion) ' Seccion
			Alertas_Inasistencias.UpdateSort(Alertas_Inasistencias.Semestre) ' Semestre
			Alertas_Inasistencias.UpdateSort(Alertas_Inasistencias.NombreProfesor) ' NombreProfesor
			Alertas_Inasistencias.UpdateSort(Alertas_Inasistencias.CODCARRERA) ' CODCARRERA
			Alertas_Inasistencias.UpdateSort(Alertas_Inasistencias.NOMBRECARRERA) ' NOMBRECARRERA
			Alertas_Inasistencias.UpdateSort(Alertas_Inasistencias.CODPLAN) ' CODPLAN
			Alertas_Inasistencias.UpdateSort(Alertas_Inasistencias.Mailenviado) ' Mailenviado
			Alertas_Inasistencias.UpdateSort(Alertas_Inasistencias.email) ' email
			sSortSql = Alertas_Inasistencias.SortSql()
			Alertas_Inasistencias.OrderBy = sSortSql
			Alertas_Inasistencias.StartGroup = 1
		End If
		Return Alertas_Inasistencias.OrderBy
	End Function

