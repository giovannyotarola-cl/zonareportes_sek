<%@ Master Language="VB" %>
<%@ Register TagPrefix="TotalMatriculados" TagName="Menu" Src="ewmenu.ascx" %>
<script runat="server">

	Public gsExport As String = ""

	Public ParentPage As AspNetReportMaker4_TotalMatriculados

	'
	' Master page Page_Init event
	'

	Protected Sub Page_Init(ByVal sender As Object, ByVal e As System.EventArgs)
		If TypeOf Me.Page Is AspNetReportMaker4_TotalMatriculados Then
			ParentPage = CType(Me.Page, AspNetReportMaker4_TotalMatriculados)
		End If
		ParentPage.StartTimer = Environment.TickCount
	End Sub

	'
	' ASP.NET Page_Load event
	'

	Protected Sub Page_Load(ByVal sender As Object, ByVal e As System.EventArgs) 
		If ParentPage.ReportLanguage Is Nothing Then
			ParentPage.ReportLanguage = New AspNetReportMaker4_TotalMatriculados.crLanguage(New AspNetReportMaker4_TotalMatriculados.AspNetReportMakerPage())
		End If
		If Request.QueryString("export") IsNot Nothing Then
			gsExport = Request.QueryString("export")
		End If
	End Sub
</script>
