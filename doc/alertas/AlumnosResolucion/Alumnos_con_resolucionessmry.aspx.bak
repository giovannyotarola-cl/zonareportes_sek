		BuildDropDownFilter(Alumnos_con_resoluciones.TIPO_SITUACION, sFilter, "")

		' Field CODIGO SITUACION
		BuildDropDownFilter(Alumnos_con_resoluciones.CODIGO_SITUACION, sFilter, "")

		' Save parms to Session
		' Field RUT

		SetSessionFilterValues(Alumnos_con_resoluciones.RUT)

		' Field NOMBRE
		SetSessionFilterValues(Alumnos_con_resoluciones.NOMBRE)

		' Field PATERNO
		SetSessionFilterValues(Alumnos_con_resoluciones.PATERNO)

		' Field MATERNO
		SetSessionFilterValues(Alumnos_con_resoluciones.MATERNO)

		' Field NOMBRE CARRERA
		SetSessionDropDownValue(Alumnos_con_resoluciones.NOMBRE_CARRERA.DropDownValue, Alumnos_con_resoluciones.NOMBRE_CARRERA)

		' Field ESTADO ACADEMICO
		SetSessionDropDownValue(Alumnos_con_resoluciones.ESTADO_ACADEMICO.DropDownValue, Alumnos_con_resoluciones.ESTADO_ACADEMICO)

		' Field AÑO DE MATRICULA
		SetSessionDropDownValue(Alumnos_con_resoluciones.AD1O_DE_dbo.DropDownValue, Alumnos_con_resoluciones.AD1O_DE_MATRICULA)

		' Field AÑO DE ULTIMA MATRICULA
		SetSessionDropDownValue(Alumnos_con_resoluciones.AD1O_DE_ULTIMA_dbo.DropDownValue, Alumnos_con_resoluciones.AD1O_DE_ULTIMA_MATRICULA)

		' Field MATRICULADO
		SetSessionDropDownValue(Alumnos_con_resoluciones.MATRICULADO.DropDownValue, Alumnos_con_resoluciones.MATRICULADO)

		' Field TIPO SITUACION
		SetSessionDropDownValue(Alumnos_con_resoluciones.TIPO_SITUACION.DropDownValue, Alumnos_con_resoluciones.TIPO_SITUACION)

		' Field CODIGO SITUACION
		SetSessionDropDownValue(Alumnos_con_resoluciones.CODIGO_SITUACION.DropDownValue, Alumnos_con_resoluciones.CODIGO_SITUACION)

		' Setup filter
		If bSetupFilter Then
			Dim sSql As String

			' Field NOMBRE CARRERA
			sWrk = ""
			BuildDropDownFilter(Alumnos_con_resoluciones.NOMBRE_CARRERA, sWrk, "")
			LoadSelectionFromFilter(Alumnos_con_resoluciones.NOMBRE_CARRERA, sWrk, Alumnos_con_resoluciones.NOMBRE_CARRERA.SelectionList)
			If (Alumnos_con_resoluciones.NOMBRE_CARRERA.SelectionList Is Nothing OrElse Alumnos_con_resoluciones.NOMBRE_CARRERA.SelectionList.Length = 0) Then
				ew_Session("sel_Alumnos_con_resoluciones_NOMBRE_CARRERA") = EWRPT_INIT_VALUE
			Else
				ew_Session("sel_Alumnos_con_resoluciones_NOMBRE_CARRERA") = Alumnos_con_resoluciones.NOMBRE_CARRERA.SelectionList
			End If
		End If
		Return sFilter
	End Function	

	' Get drop down value from querystring 
	Public Function GetDropDownValue(ByRef fld As crField) As Boolean
		If HttpContext.Current.Request.Form.Count > 0 Then
			Return False
		End If

		' Skip post back
		Dim parm As String = fld.FldVar.Substring(2)
		If ew_NotEmpty(ew_Get("sv_" + parm)) Then
			Dim sv As Object = HttpContext.Current.Request.QueryString.GetValues("sv_" + parm)
			If DirectCast(sv, String()).Length = 1 Then
				sv = ew_Get("sv_" + parm)
			End If
			fld.DropDownValue = sv
			Return True
		End If
		Return False
	End Function

	' Get filter values from querystring 
	Public Function GetFilterValues(ByRef fld As crField) As Boolean
		Dim parm As String = fld.FldVar.Substring(2)
		Dim ReturnValue As Boolean = False

		' Skip post back 
		If HttpContext.Current.Request.Form.Count > 0 Then	Return ReturnValue
		If HttpContext.Current.Request.QueryString("sv1_" & parm) IsNot Nothing Then
			fld.SearchValue = ew_Get("sv1_" & parm)
			ReturnValue = True
		End If
		If HttpContext.Current.Request.QueryString("so1_" & parm) IsNot Nothing Then
			fld.SearchOperator = ew_Get("so1_" & parm)
			ReturnValue = True
		End If
		If HttpContext.Current.Request.QueryString("sc_" & parm) IsNot Nothing Then
			fld.SearchCondition = ew_Get("sc_" & parm)
			ReturnValue = True
		End If
		If HttpContext.Current.Request.QueryString("sv2_" & parm) IsNot Nothing Then
			fld.SearchValue2 = ew_Get("sv2_" & parm)
			ReturnValue = True
		End If
		If HttpContext.Current.Request.QueryString("so2_" & parm) IsNot Nothing Then
			fld.SearchOperator2 = ew_Get("so2_" & parm)
			ReturnValue = True
		End If
		Return ReturnValue
	End Function

			' Set default ext filter
	Public Sub SetDefaultExtFilter(ByRef fld As crField, ByVal so1 As String, ByVal sv1 As String, ByVal sc As String, ByVal so2 As String, ByVal sv2 As String)
		fld.DefaultSearchValue = sv1 ' Default ext filter value 1
		fld.DefaultSearchOperator = so1	' Default search operator 1
		fld.DefaultSearchCondition = sc	' Default search condition (if operator 2 is enabled)
