	' Return Sort parameters based on Sort links clicked
	Public Function GetSort() As String
		Dim bCtrl As Boolean = False
		Dim sOrder As String, sOrderType As String, sOrderBy As String
		Dim sSortField As String, sLastSort As String, sThisSort As String = "", sSortSql As String = ""

		' Check for a resetsort command
		If ew_NotEmpty(ew_Get("cmd")) Then
			Dim sCmd As String = ew_Get("cmd")
			If ew_SameText(sCmd, "resetsort") Then
				Alumnos_con_resoluciones.OrderBy = ""
				Alumnos_con_resoluciones.StartGroup = 1
				Alumnos_con_resoluciones.RUT.Sort = ""
				Alumnos_con_resoluciones.CODIGO_ALUMNO.Sort = ""
				Alumnos_con_resoluciones.NOMBRE.Sort = ""
				Alumnos_con_resoluciones.PATERNO.Sort = ""
				Alumnos_con_resoluciones.MATERNO.Sort = ""
				Alumnos_con_resoluciones.CODIGO_CARRERA.Sort = ""
				Alumnos_con_resoluciones.NOMBRE_CARRERA.Sort = ""
				Alumnos_con_resoluciones.ESTADO_ACADEMICO.Sort = ""
				Alumnos_con_resoluciones.AD1O_DE_dbo.Sort = ""
				Alumnos_con_resoluciones.AD1O_DE_ULTIMA_dbo.Sort = ""
				Alumnos_con_resoluciones.MATRICULADO.Sort = ""
				Alumnos_con_resoluciones.TIPO_SITUACION.Sort = ""
				Alumnos_con_resoluciones.CODIGO_SITUACION.Sort = ""
				Alumnos_con_resoluciones.OBSERVACION.Sort = ""
			End If

		' Check for an Order parameter
		ElseIf (ew_NotEmpty(ew_Get("order"))) Then
			Alumnos_con_resoluciones.CurrentOrder = ew_Get("order")
			Alumnos_con_resoluciones.CurrentOrderType = ew_Get("ordertype")
			sSortSql = Alumnos_con_resoluciones.SortSql()
			Alumnos_con_resoluciones.OrderBy = sSortSql
			Alumnos_con_resoluciones.StartGroup = 1
		End If

		' Set up default sort
		If (ew_Empty(Alumnos_con_resoluciones.OrderBy)) Then
			Alumnos_con_resoluciones.OrderBy = "[PATERNO] ASC, [MATERNO] ASC, [NOMBRE] ASC"
			Alumnos_con_resoluciones.PATERNO.Sort = "ASC"
			Alumnos_con_resoluciones.MATERNO.Sort = "ASC"
			Alumnos_con_resoluciones.NOMBRE.Sort = "ASC"
		End If
		Return Alumnos_con_resoluciones.OrderBy
	End Function

