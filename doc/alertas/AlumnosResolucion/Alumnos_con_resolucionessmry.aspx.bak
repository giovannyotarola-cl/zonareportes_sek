			' Field AÑO DE MATRICULA
			Alumnos_con_resoluciones.AD1O_DE_dbo.DefaultDropDownValue = EWRPT_INIT_VALUE
			Alumnos_con_resoluciones.AD1O_DE_dbo.DropDownValue = Alumnos_con_resoluciones.AD1O_DE_dbo.DefaultDropDownValue

			' Field AÑO DE ULTIMA MATRICULA
			Alumnos_con_resoluciones.AD1O_DE_ULTIMA_dbo.DefaultDropDownValue = EWRPT_INIT_VALUE
			Alumnos_con_resoluciones.AD1O_DE_ULTIMA_dbo.DropDownValue = Alumnos_con_resoluciones.AD1O_DE_ULTIMA_dbo.DefaultDropDownValue

			' Field MATRICULADO
			Alumnos_con_resoluciones.MATRICULADO.DefaultDropDownValue = EWRPT_INIT_VALUE
			Alumnos_con_resoluciones.MATRICULADO.DropDownValue = Alumnos_con_resoluciones.MATRICULADO.DefaultDropDownValue

			' Field TIPO SITUACION
			Alumnos_con_resoluciones.TIPO_SITUACION.DefaultDropDownValue = EWRPT_INIT_VALUE
			Alumnos_con_resoluciones.TIPO_SITUACION.DropDownValue = Alumnos_con_resoluciones.TIPO_SITUACION.DefaultDropDownValue

