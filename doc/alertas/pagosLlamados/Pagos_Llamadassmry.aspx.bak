				If (GetDropDownValue(Pagos_Llamadas.PERIODO)) Then
					bSetupFilter = True ' Set up filter required
					bRestoreSession = False ' Do not restore from session
				ElseIf Not ew_IsArrayList(Pagos_Llamadas.PERIODO.DropDownValue) Then
					If Not ew_SameStr(Pagos_Llamadas.PERIODO.DropDownValue, EWRPT_INIT_VALUE) AndAlso ew_Session("sv_Pagos_Llamadas_PERIODO") Is Nothing Then
						bSetupFilter = True ' Set up filter required
					End If
				End If

				' Field ANO_MATRICULA
				If (GetDropDownValue(Pagos_Llamadas.ANO_MATRICULA)) Then
					bSetupFilter = True ' Set up filter required
					bRestoreSession = False ' Do not restore from session
				ElseIf Not ew_IsArrayList(Pagos_Llamadas.ANO_dbo.DropDownValue) Then
					If Not ew_SameStr(Pagos_Llamadas.ANO_dbo.DropDownValue, EWRPT_INIT_VALUE) AndAlso ew_Session("sv_Pagos_Llamadas_ANO_MATRICULA") Is Nothing Then
						bSetupFilter = True ' Set up filter required
					End If
				End If

				' Field ANO_ULT_MAT
				If (GetDropDownValue(Pagos_Llamadas.ANO_ULT_MAT)) Then
					bSetupFilter = True ' Set up filter required
					bRestoreSession = False ' Do not restore from session
				ElseIf Not ew_IsArrayList(Pagos_Llamadas.ANO_ULT_MAT.DropDownValue) Then
					If Not ew_SameStr(Pagos_Llamadas.ANO_ULT_MAT.DropDownValue, EWRPT_INIT_VALUE) AndAlso ew_Session("sv_Pagos_Llamadas_ANO_ULT_MAT") Is Nothing Then
						bSetupFilter = True ' Set up filter required
					End If
				End If

				' Field PER_ULT_MAT
				If (GetDropDownValue(Pagos_Llamadas.PER_ULT_MAT)) Then
					bSetupFilter = True ' Set up filter required
					bRestoreSession = False ' Do not restore from session
				ElseIf Not ew_IsArrayList(Pagos_Llamadas.PER_ULT_MAT.DropDownValue) Then
					If Not ew_SameStr(Pagos_Llamadas.PER_ULT_MAT.DropDownValue, EWRPT_INIT_VALUE) AndAlso ew_Session("sv_Pagos_Llamadas_PER_ULT_MAT") Is Nothing Then
						bSetupFilter = True ' Set up filter required
					End If
				End If

				' Field DOCUM_PAGO
				If (GetDropDownValue(Pagos_Llamadas.DOCUM_PAGO)) Then
					bSetupFilter = True ' Set up filter required
					bRestoreSession = False ' Do not restore from session
				ElseIf Not ew_IsArrayList(Pagos_Llamadas.DOCUM_PAGO.DropDownValue) Then
					If Not ew_SameStr(Pagos_Llamadas.DOCUM_PAGO.DropDownValue, EWRPT_INIT_VALUE) AndAlso ew_Session("sv_Pagos_Llamadas_DOCUM_PAGO") Is Nothing Then
						bSetupFilter = True ' Set up filter required
					End If
				End If
			If Not ValidateForm() Then
				Message = gsFormError
				Return sFilter
			End If
		End If

		' Restore session
		If bRestoreSession Then

			' Field CODCLI
			GetSessionFilterValues(Pagos_Llamadas.CODCLI)

			' Field NOMBRE
			GetSessionFilterValues(Pagos_Llamadas.NOMBRE)

			' Field PATERNO
			GetSessionFilterValues(Pagos_Llamadas.PATERNO)

			' Field MATERNO
			GetSessionFilterValues(Pagos_Llamadas.MATERNO)

			' Field CODCARR
			GetSessionDropDownValue(Pagos_Llamadas.CODCARR)

			' Field NOMBRE_C
			GetSessionDropDownValue(Pagos_Llamadas.NOMBRE_C)

			' Field CODCARPR
			GetSessionDropDownValue(Pagos_Llamadas.CODCARPR)

			' Field ESTACAD
			GetSessionDropDownValue(Pagos_Llamadas.ESTACAD)

			' Field ANO
			GetSessionDropDownValue(Pagos_Llamadas.ANO)

			' Field PERIODO
			GetSessionDropDownValue(Pagos_Llamadas.PERIODO)

			' Field ANO_MATRICULA
			GetSessionDropDownValue(Pagos_Llamadas.ANO_MATRICULA)

			' Field ANO_ULT_MAT
			GetSessionDropDownValue(Pagos_Llamadas.ANO_ULT_MAT)

			' Field PER_ULT_MAT
			GetSessionDropDownValue(Pagos_Llamadas.PER_ULT_MAT)

			' Field DOCUM_PAGO
			GetSessionDropDownValue(Pagos_Llamadas.DOCUM_PAGO)
		End If

		' Call page filter validated event
		Pagos_Llamadas.Page_FilterValidated()

		' Build SQL
		' Field CODCLI

		BuildExtendedFilter(Pagos_Llamadas.CODCLI, sFilter)

		' Field NOMBRE
		BuildExtendedFilter(Pagos_Llamadas.NOMBRE, sFilter)

		' Field PATERNO
		BuildExtendedFilter(Pagos_Llamadas.PATERNO, sFilter)

		' Field MATERNO
		BuildExtendedFilter(Pagos_Llamadas.MATERNO, sFilter)

		' Field CODCARR
		BuildDropDownFilter(Pagos_Llamadas.CODCARR, sFilter, "")

		' Field NOMBRE_C
		BuildDropDownFilter(Pagos_Llamadas.NOMBRE_C, sFilter, "")

		' Field CODCARPR
		BuildDropDownFilter(Pagos_Llamadas.CODCARPR, sFilter, "")

		' Field ESTACAD
		BuildDropDownFilter(Pagos_Llamadas.ESTACAD, sFilter, "")

		' Field ANO
		BuildDropDownFilter(Pagos_Llamadas.ANO, sFilter, "")

		' Field PERIODO
		BuildDropDownFilter(Pagos_Llamadas.PERIODO, sFilter, "")

		' Field ANO_MATRICULA
		BuildDropDownFilter(Pagos_Llamadas.ANO_MATRICULA, sFilter, "")

		' Field ANO_ULT_MAT
		BuildDropDownFilter(Pagos_Llamadas.ANO_ULT_MAT, sFilter, "")

		' Field PER_ULT_MAT
		BuildDropDownFilter(Pagos_Llamadas.PER_ULT_MAT, sFilter, "")

		' Field DOCUM_PAGO
		BuildDropDownFilter(Pagos_Llamadas.DOCUM_PAGO, sFilter, "")

		' Save parms to Session
		' Field CODCLI

		SetSessionFilterValues(Pagos_Llamadas.CODCLI)

		' Field NOMBRE
		SetSessionFilterValues(Pagos_Llamadas.NOMBRE)

		' Field PATERNO
		SetSessionFilterValues(Pagos_Llamadas.PATERNO)

		' Field MATERNO
		SetSessionFilterValues(Pagos_Llamadas.MATERNO)

		' Field CODCARR
		SetSessionDropDownValue(Pagos_Llamadas.CODCARR.DropDownValue, Pagos_Llamadas.CODCARR)

		' Field NOMBRE_C
		SetSessionDropDownValue(Pagos_Llamadas.NOMBRE_C.DropDownValue, Pagos_Llamadas.NOMBRE_C)

		' Field CODCARPR
		SetSessionDropDownValue(Pagos_Llamadas.CODCARPR.DropDownValue, Pagos_Llamadas.CODCARPR)

		' Field ESTACAD
		SetSessionDropDownValue(Pagos_Llamadas.ESTACAD.DropDownValue, Pagos_Llamadas.ESTACAD)

		' Field ANO
		SetSessionDropDownValue(Pagos_Llamadas.ANO.DropDownValue, Pagos_Llamadas.ANO)

		' Field PERIODO
		SetSessionDropDownValue(Pagos_Llamadas.PERIODO.DropDownValue, Pagos_Llamadas.PERIODO)

		' Field ANO_MATRICULA
		SetSessionDropDownValue(Pagos_Llamadas.ANO_dbo.DropDownValue, Pagos_Llamadas.ANO_MATRICULA)

		' Field ANO_ULT_MAT
		SetSessionDropDownValue(Pagos_Llamadas.ANO_ULT_MAT.DropDownValue, Pagos_Llamadas.ANO_ULT_MAT)

		' Field PER_ULT_MAT
		SetSessionDropDownValue(Pagos_Llamadas.PER_ULT_MAT.DropDownValue, Pagos_Llamadas.PER_ULT_MAT)

		' Field DOCUM_PAGO
		SetSessionDropDownValue(Pagos_Llamadas.DOCUM_PAGO.DropDownValue, Pagos_Llamadas.DOCUM_PAGO)

		' Setup filter
		If bSetupFilter Then
			Dim sSql As String

			' Field PATERNO
			sWrk = ""
			BuildExtendedFilter(Pagos_Llamadas.PATERNO, sWrk)
			LoadSelectionFromFilter(Pagos_Llamadas.PATERNO, sWrk, Pagos_Llamadas.PATERNO.SelectionList)
			If (Pagos_Llamadas.PATERNO.SelectionList Is Nothing OrElse Pagos_Llamadas.PATERNO.SelectionList.Length = 0) Then
				ew_Session("sel_Pagos_Llamadas_PATERNO") = EWRPT_INIT_VALUE
			Else
				ew_Session("sel_Pagos_Llamadas_PATERNO") = Pagos_Llamadas.PATERNO.SelectionList
			End If

			' Field MATERNO
			sWrk = ""
			BuildExtendedFilter(Pagos_Llamadas.MATERNO, sWrk)
			LoadSelectionFromFilter(Pagos_Llamadas.MATERNO, sWrk, Pagos_Llamadas.MATERNO.SelectionList)
			If (Pagos_Llamadas.MATERNO.SelectionList Is Nothing OrElse Pagos_Llamadas.MATERNO.SelectionList.Length = 0) Then
				ew_Session("sel_Pagos_Llamadas_MATERNO") = EWRPT_INIT_VALUE
			Else
				ew_Session("sel_Pagos_Llamadas_MATERNO") = Pagos_Llamadas.MATERNO.SelectionList
			End If

			' Field NOMBRE_C
			sWrk = ""
			BuildDropDownFilter(Pagos_Llamadas.NOMBRE_C, sWrk, "")
			LoadSelectionFromFilter(Pagos_Llamadas.NOMBRE_C, sWrk, Pagos_Llamadas.NOMBRE_C.SelectionList)
			If (Pagos_Llamadas.NOMBRE_C.SelectionList Is Nothing OrElse Pagos_Llamadas.NOMBRE_C.SelectionList.Length = 0) Then
				ew_Session("sel_Pagos_Llamadas_NOMBRE_C") = EWRPT_INIT_VALUE
			Else
				ew_Session("sel_Pagos_Llamadas_NOMBRE_C") = Pagos_Llamadas.NOMBRE_C.SelectionList
			End If
		End If
		Return sFilter
	End Function	

	' Get drop down value from querystring 
	Public Function GetDropDownValue(ByRef fld As crField) As Boolean
		If HttpContext.Current.Request.Form.Count > 0 Then
			Return False
		End If

		' Skip post back
		Dim parm As String = fld.FldVar.Substring(2)
		If ew_NotEmpty(ew_Get("sv_" + parm)) Then
			Dim sv As Object = HttpContext.Current.Request.QueryString.GetValues("sv_" + parm)
			If DirectCast(sv, String()).Length = 1 Then
				sv = ew_Get("sv_" + parm)
			End If
			fld.DropDownValue = sv
			Return True
		End If
		Return False
	End Function

	' Get filter values from querystring 
	Public Function GetFilterValues(ByRef fld As crField) As Boolean
		Dim parm As String = fld.FldVar.Substring(2)
		Dim ReturnValue As Boolean = False

		' Skip post back 
		If HttpContext.Current.Request.Form.Count > 0 Then	Return ReturnValue
		If HttpContext.Current.Request.QueryString("sv1_" & parm) IsNot Nothing Then
			fld.SearchValue = ew_Get("sv1_" & parm)
			ReturnValue = True
		End If
		If HttpContext.Current.Request.QueryString("so1_" & parm) IsNot Nothing Then
			fld.SearchOperator = ew_Get("so1_" & parm)
			ReturnValue = True
		End If
		If HttpContext.Current.Request.QueryString("sc_" & parm) IsNot Nothing Then
			fld.SearchCondition = ew_Get("sc_" & parm)
			ReturnValue = True
		End If
		If HttpContext.Current.Request.QueryString("sv2_" & parm) IsNot Nothing Then
			fld.SearchValue2 = ew_Get("sv2_" & parm)
			ReturnValue = True
		End If
		If HttpContext.Current.Request.QueryString("so2_" & parm) IsNot Nothing Then
			fld.SearchOperator2 = ew_Get("so2_" & parm)
			ReturnValue = True
		End If
		Return ReturnValue
	End Function

			' Set default ext filter
	Public Sub SetDefaultExtFilter(ByRef fld As crField, ByVal so1 As String, ByVal sv1 As String, ByVal sc As String, ByVal so2 As String, ByVal sv2 As String)
		fld.DefaultSearchValue = sv1 ' Default ext filter value 1
		fld.DefaultSearchOperator = so1	' Default search operator 1
		fld.DefaultSearchCondition = sc	' Default search condition (if operator 2 is enabled)
		fld.DefaultSearchValue2 = sv2	' Default ext filter value 2 (if operator 2 is enabled)
		fld.DefaultSearchOperator2 = so2 ' Default search operator 2 (if operator 2 is enabled)
	End Sub

	' Apply default ext filter
	Public Sub ApplyDefaultExtFilter(ByRef fld As crField)
		fld.SearchValue = fld.DefaultSearchValue
		fld.SearchOperator = fld.DefaultSearchOperator
		fld.SearchCondition = fld.DefaultSearchCondition
		fld.SearchValue2 = fld.DefaultSearchValue2
		fld.SearchOperator2 = fld.DefaultSearchOperator2
	End Sub

	' Check if Text Filter applied
	Public Function TextFilterApplied(ByRef fld As crField) As Boolean
		Return (Not ew_SameStr(fld.SearchValue, fld.DefaultSearchValue) OrElse Not ew_SameStr(fld.SearchValue2, fld.DefaultSearchValue2) OrElse (ew_NotEmpty(fld.SearchValue) AndAlso Not ew_SameStr(fld.SearchOperator, fld.DefaultSearchOperator)) OrElse (ew_NotEmpty(fld.SearchValue2) AndAlso Not ew_SameStr(fld.SearchOperator2, fld.DefaultSearchOperator2)) OrElse Not ew_SameStr(fld.SearchCondition, fld.DefaultSearchCondition))
	End Function

	' Check if Non-Text Filter applied
	Public Function NonTextFilterApplied(ByRef fld As crField) As Boolean
		If ewrpt_IsArray(fld.DefaultDropDownValue) AndAlso ewrpt_IsArray(fld.DropDownValue) Then
			Dim ar1 As String() = DirectCast(fld.DropDownValue, String())
			Dim ar2 As String() = DirectCast(fld.DefaultDropDownValue, String())
			If ar1.Length <> ar2.Length Then
				Return True
			Else
				Array.Sort(ar1)
				Array.Sort(ar2)
				Return Not ewrpt_MatchedArray(ar1, ar2)
			End If
		ElseIf ewrpt_IsArray(fld.DefaultDropDownValue) OrElse ewrpt_IsArray(fld.DropDownValue) Then
			Return True
		Else
			Dim v1 As String = Convert.ToString(fld.DefaultDropDownValue)
			If v1 = EWRPT_INIT_VALUE Then
				v1 = ""
			End If
			Dim v2 As String = Convert.ToString(fld.DropDownValue)
			If v2 = EWRPT_INIT_VALUE OrElse v2 = EWRPT_ALL_VALUE Then
				v2 = ""
			End If
			Return (Not ew_SameStr(v1, v2))
		End If
	End Function

	' Load selection from a filter clause
	Public Sub LoadSelectionFromFilter(ByRef fld As crField, ByVal filter As String, ByRef sel As ArrayList)
		sel.Clear()
		If ew_NotEmpty(filter) Then
			Dim sSql As String = ewrpt_BuildReportSql(fld.SqlSelect, "", "", "", fld.SqlOrderBy, filter, "")
			ParentPage.ewrpt_LoadArrayListFromSql(sSql, sel)			
		End If
	End Sub

	' Load selection from a filter clause
	Public Sub LoadSelectionFromFilter(ByRef fld As crField, ByVal filter As String, ByRef sel As String())
		sel = New String(-1) {}
		If ew_NotEmpty(filter) Then
			Dim sSql As String = ewrpt_BuildReportSql(fld.SqlSelect, "", "", "", fld.SqlOrderBy, filter, "")
			ParentPage.ewrpt_LoadArrayFromSql(sSql, sel)
		End If
	End Sub

