	' Check if filter applied
	Public Function CheckFilter() As Boolean
	  Dim bFilterExist As Boolean = False

		' Check CODCLI extended filter
		If (TextFilterApplied(Pagos_Llamadas.CODCLI)) Then bFilterExist = True

		' Check NOMBRE extended filter
		If (TextFilterApplied(Pagos_Llamadas.NOMBRE)) Then bFilterExist = True

		' Check PATERNO extended filter
		If (TextFilterApplied(Pagos_Llamadas.PATERNO)) Then bFilterExist = True

		' Check PATERNO popup filter
		If Not ewrpt_MatchedArray(CTYpe(Pagos_Llamadas.PATERNO.DefaultSelectionList.ToArray(GetType(String)), String()), Pagos_Llamadas.PATERNO.SelectionList) Then bFilterExist = True

		' Check MATERNO extended filter
		If (TextFilterApplied(Pagos_Llamadas.MATERNO)) Then bFilterExist = True

		' Check MATERNO popup filter
		If Not ewrpt_MatchedArray(CTYpe(Pagos_Llamadas.MATERNO.DefaultSelectionList.ToArray(GetType(String)), String()), Pagos_Llamadas.MATERNO.SelectionList) Then bFilterExist = True

		' Check CODCARR dropdown filter
		If (NonTextFilterApplied(Pagos_Llamadas.CODCARR)) Then bFilterExist = True

		' Check NOMBRE_C dropdown filter
		If (NonTextFilterApplied(Pagos_Llamadas.NOMBRE_C)) Then bFilterExist = True

		' Check NOMBRE_C popup filter
		If Not ewrpt_MatchedArray(CTYpe(Pagos_Llamadas.NOMBRE_C.DefaultSelectionList.ToArray(GetType(String)), String()), Pagos_Llamadas.NOMBRE_C.SelectionList) Then bFilterExist = True

		' Check CODCARPR dropdown filter
		If (NonTextFilterApplied(Pagos_Llamadas.CODCARPR)) Then bFilterExist = True

		' Check ESTACAD dropdown filter
		If (NonTextFilterApplied(Pagos_Llamadas.ESTACAD)) Then bFilterExist = True

		' Check ANO dropdown filter
		If (NonTextFilterApplied(Pagos_Llamadas.ANO)) Then bFilterExist = True

		' Check PERIODO dropdown filter
		If (NonTextFilterApplied(Pagos_Llamadas.PERIODO)) Then bFilterExist = True

		' Check ANO_MATRICULA dropdown filter
		If (NonTextFilterApplied(Pagos_Llamadas.ANO_MATRICULA)) Then bFilterExist = True

		' Check ANO_ULT_MAT dropdown filter
		If (NonTextFilterApplied(Pagos_Llamadas.ANO_ULT_MAT)) Then bFilterExist = True

		' Check PER_ULT_MAT dropdown filter
		If (NonTextFilterApplied(Pagos_Llamadas.PER_ULT_MAT)) Then bFilterExist = True

		' Check DOCUM_PAGO dropdown filter
		If (NonTextFilterApplied(Pagos_Llamadas.DOCUM_PAGO)) Then bFilterExist = True
		Return bFilterExist
	End Function	

