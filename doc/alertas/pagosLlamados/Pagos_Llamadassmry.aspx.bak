	' Return extended filter
	Public Function GetExtendedFilter() As String
		Dim sFilter As String = ""
	  Dim sWrk As String
	  Dim bPostBack As Boolean = (HttpContext.Current.Request.Form.Count > 0)
	  Dim bRestoreSession As Boolean = True
		Dim bSetupFilter As Boolean = False		
		If bPostBack Then ' Reset extended filter if filter changed

			' Clear extended filter for field PATERNO
			If ClearExtFilter = "Pagos_Llamadas_PATERNO" Then
				ClearSessionFilterValues(Pagos_Llamadas.PATERNO)
			End If

			' Clear extended filter for field MATERNO
			If ClearExtFilter = "Pagos_Llamadas_MATERNO" Then
				ClearSessionFilterValues(Pagos_Llamadas.MATERNO)
			End If

			' Clear dropdown for field NOMBRE_C
			If ClearExtFilter = "Pagos_Llamadas_NOMBRE_C" Then
				SetSessionDropDownValue(EWRPT_INIT_VALUE, Pagos_Llamadas.NOMBRE_C)
			End If
		ElseIf ew_SameText(ew_Get("cmd"), "reset") Then ' Reset search command

				' Field CODCLI
				SetSessionFilterValues(Pagos_Llamadas.CODCLI)

				' Field NOMBRE
				SetSessionFilterValues(Pagos_Llamadas.NOMBRE)

				' Field PATERNO
				SetSessionFilterValues(Pagos_Llamadas.PATERNO)

				' Field MATERNO
				SetSessionFilterValues(Pagos_Llamadas.MATERNO)

				' Field CODCARR
				SetSessionDropDownValue(Pagos_Llamadas.CODCARR.DropDownValue, Pagos_Llamadas.CODCARR)

				' Field NOMBRE_C
				SetSessionDropDownValue(Pagos_Llamadas.NOMBRE_C.DropDownValue, Pagos_Llamadas.NOMBRE_C)

				' Field CODCARPR
				SetSessionDropDownValue(Pagos_Llamadas.CODCARPR.DropDownValue, Pagos_Llamadas.CODCARPR)

				' Field ESTACAD
				SetSessionDropDownValue(Pagos_Llamadas.ESTACAD.DropDownValue, Pagos_Llamadas.ESTACAD)

				' Field ANO
				SetSessionDropDownValue(Pagos_Llamadas.ANO.DropDownValue, Pagos_Llamadas.ANO)

