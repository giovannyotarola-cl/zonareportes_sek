	' Return popup filter
	Public Function GetPopupFilter() As String
		Dim sWrk As String = ""
		If Not ExtendedFilterExist(Pagos_Llamadas.PATERNO) Then
			If (ewrpt_IsArray(Pagos_Llamadas.PATERNO.SelectionList) AndAlso Pagos_Llamadas.PATERNO.SelectionList.Length > 0) Then
				If ew_NotEmpty(sWrk) Then sWrk &= " AND "
				sWrk += ewrpt_FilterSQL(Pagos_Llamadas.PATERNO, "[PATERNO]", EWRPT_DATATYPE_STRING)
			End If		
		End If
		If Not ExtendedFilterExist(Pagos_Llamadas.MATERNO) Then
			If (ewrpt_IsArray(Pagos_Llamadas.MATERNO.SelectionList) AndAlso Pagos_Llamadas.MATERNO.SelectionList.Length > 0) Then
				If ew_NotEmpty(sWrk) Then sWrk &= " AND "
				sWrk += ewrpt_FilterSQL(Pagos_Llamadas.MATERNO, "[MATERNO]", EWRPT_DATATYPE_STRING)
			End If		
		End If
		If Not DropDownFilterExist(Pagos_Llamadas.NOMBRE_C, "") Then
			If (ewrpt_IsArray(Pagos_Llamadas.NOMBRE_C.SelectionList) AndAlso Pagos_Llamadas.NOMBRE_C.SelectionList.Length > 0) Then
				If ew_NotEmpty(sWrk) Then sWrk &= " AND "
				sWrk += ewrpt_FilterSQL(Pagos_Llamadas.NOMBRE_C, "[NOMBRE_C]", EWRPT_DATATYPE_STRING)
			End If		
		End If
		Return sWrk
	End Function

	' Return Sort parameters based on Sort links clicked
	Public Function GetSort() As String
		Dim bCtrl As Boolean = False
		Dim sOrder As String, sOrderType As String, sOrderBy As String
		Dim sSortField As String, sLastSort As String, sThisSort As String = "", sSortSql As String = ""

		' Check for a resetsort command
		If ew_NotEmpty(ew_Get("cmd")) Then
			Dim sCmd As String = ew_Get("cmd")
			If ew_SameText(sCmd, "resetsort") Then
				Pagos_Llamadas.OrderBy = ""
				Pagos_Llamadas.StartGroup = 1
				Pagos_Llamadas.CODCLI.Sort = ""
				Pagos_Llamadas.DIG.Sort = ""
				Pagos_Llamadas.NOMBRE.Sort = ""
				Pagos_Llamadas.PATERNO.Sort = ""
				Pagos_Llamadas.MATERNO.Sort = ""
				Pagos_Llamadas.CODCARR.Sort = ""
				Pagos_Llamadas.NOMBRE_C.Sort = ""
				Pagos_Llamadas.CODCARPR.Sort = ""
				Pagos_Llamadas.ESTACAD.Sort = ""
				Pagos_Llamadas.ANO.Sort = ""
				Pagos_Llamadas.PERIODO.Sort = ""
				Pagos_Llamadas.ANO_dbo.Sort = ""
				Pagos_Llamadas.ANO_ULT_MAT.Sort = ""
				Pagos_Llamadas.PER_ULT_MAT.Sort = ""
				Pagos_Llamadas.SALDO.Sort = ""
				Pagos_Llamadas.total.Sort = ""
				Pagos_Llamadas.FECCANCEL.Sort = ""
				Pagos_Llamadas.DOCUM_PAGO.Sort = ""
			End If

		' Check for an Order parameter
		ElseIf (ew_NotEmpty(ew_Get("order"))) Then
			Pagos_Llamadas.CurrentOrder = ew_Get("order")
			Pagos_Llamadas.CurrentOrderType = ew_Get("ordertype")
			sSortSql = Pagos_Llamadas.SortSql()
			Pagos_Llamadas.OrderBy = sSortSql
			Pagos_Llamadas.StartGroup = 1
		End If

		' Set up default sort
		If (ew_Empty(Pagos_Llamadas.OrderBy)) Then
			Pagos_Llamadas.OrderBy = "[PATERNO] ASC, [MATERNO] ASC"
			Pagos_Llamadas.PATERNO.Sort = "ASC"
			Pagos_Llamadas.MATERNO.Sort = "ASC"
		End If
		Return Pagos_Llamadas.OrderBy
	End Function

