				' Field DOCUM_PAGO
				SetSessionDropDownValue(Pagos_Llamadas.DOCUM_PAGO.DropDownValue, Pagos_Llamadas.DOCUM_PAGO)
			bSetupFilter = True ' Set up filter required
		Else

				' Field CODCLI
				If (GetFilterValues(Pagos_Llamadas.CODCLI)) Then
					bSetupFilter = True ' Set up filter required
					bRestoreSession = False ' Do not restore from session
				End If

