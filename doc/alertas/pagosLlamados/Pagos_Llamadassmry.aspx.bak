							arValues = EWRPT_INIT_VALUE
						End If
						If Not ewrpt_MatchedArray(arValues, ew_Session("sel_" + sName)) Then
							If (HasSessionFilterValues(sName)) Then
								ClearExtFilter = sName ' Clear extended filter for this field
							End If
						End If
						ew_Session("sel_" + sName) = arValues
						ew_Session("rf_" + sName) = ew_Post("rf_" + sName)
						ew_Session("rt_" + sName) = ew_Post("rt_" + sName)
						ResetPager()
					End If
				End If

			' Get 'reset' command
			ElseIf (ew_NotEmpty(ew_Get("cmd"))) Then '  Get reset cmd
				Dim sCmd As String = ew_Get("cmd")
				If (ew_SameText(sCmd, "reset")) Then
					ClearSessionSelection("PATERNO")
					ClearSessionSelection("MATERNO")
					ClearSessionSelection("NOMBRE_C")
					ResetPager()
				End If
			End If

			' Load selection criteria to array
			' Get PATERNO selected values

			If (ewrpt_IsArray(ew_Session("sel_Pagos_Llamadas_PATERNO"))) Then
				LoadSelectionFromSession("PATERNO")
			ElseIf (ew_SameStr(ew_Session("sel_Pagos_Llamadas_PATERNO"), EWRPT_INIT_VALUE)) Then ' Select all
				Pagos_Llamadas.PATERNO.SelectionList = Nothing
			End If

			' Get MATERNO selected values
			If (ewrpt_IsArray(ew_Session("sel_Pagos_Llamadas_MATERNO"))) Then
				LoadSelectionFromSession("MATERNO")
			ElseIf (ew_SameStr(ew_Session("sel_Pagos_Llamadas_MATERNO"), EWRPT_INIT_VALUE)) Then ' Select all
				Pagos_Llamadas.MATERNO.SelectionList = Nothing
			End If

			' Get NOMBRE C selected values
			If (ewrpt_IsArray(ew_Session("sel_Pagos_Llamadas_NOMBRE_C"))) Then
				LoadSelectionFromSession("NOMBRE_C")
			ElseIf (ew_SameStr(ew_Session("sel_Pagos_Llamadas_NOMBRE_C"), EWRPT_INIT_VALUE)) Then ' Select all
				Pagos_Llamadas.NOMBRE_C.SelectionList = Nothing
			End If
		End Sub

		' Reset pager to starting position
		Public Sub ResetPager()
			StartGrp = 1
			Pagos_Llamadas.StartGroup = StartGrp
		End Sub

		Public Pager As cPrevNextPager

		' Set up number of groups displayed per page based on form element GrpPerPage 
		Public Sub SetUpDisplayGrps()
			Dim sWrk As String = ew_Get(EWRPT_TABLE_GROUP_PER_PAGE)
			If (ew_NotEmpty(sWrk)) Then 
				If (ewrpt_IsNumeric(sWrk)) Then 
					DisplayGrps = ew_ConvertToInt(sWrk) 
				Else 
					If (ew_SameText(sWrk, "ALL")) Then ' Display all records 
						DisplayGrps = -1 
					Else 
						DisplayGrps = 20 ' Non-numeric, load default 
					End If 
				End If				
				Pagos_Llamadas.GroupPerPage = DisplayGrps ' Save to session
				StartGrp = 1 ' Reset start position (reset command)				
				Pagos_Llamadas.StartGroup = StartGrp
			Else				
				If (Pagos_Llamadas.GroupPerPage <> 0) Then 
					DisplayGrps = ew_ConvertToInt(Pagos_Llamadas.GroupPerPage) ' Restore from Session 
				Else 
					DisplayGrps = 20 ' Load default 
				End If 
			End If 
		End Sub 

