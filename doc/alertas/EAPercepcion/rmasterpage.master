<%@ Master Language="VB" %>
<%@ Register TagPrefix="E_Percepcion" TagName="Menu" Src="ewmenu.ascx" %>
<script runat="server">

	Public gsExport As String = ""

	Public ParentPage As AspNetReportMaker4_E_Percepcion

	'
	' Master page Page_Init event
	'

	Protected Sub Page_Init(ByVal sender As Object, ByVal e As System.EventArgs)
		If TypeOf Me.Page Is AspNetReportMaker4_E_Percepcion Then
			ParentPage = CType(Me.Page, AspNetReportMaker4_E_Percepcion)
		End If
		ParentPage.StartTimer = Environment.TickCount
	End Sub

	'
	' ASP.NET Page_Load event
	'

	Protected Sub Page_Load(ByVal sender As Object, ByVal e As System.EventArgs) 
		If ParentPage.ReportLanguage Is Nothing Then
			ParentPage.ReportLanguage = New AspNetReportMaker4_E_Percepcion.crLanguage(New AspNetReportMaker4_E_Percepcion.AspNetReportMakerPage())
		End If
		If Request.QueryString("export") IsNot Nothing Then
			gsExport = Request.QueryString("export")
		End If
	End Sub
</script>
